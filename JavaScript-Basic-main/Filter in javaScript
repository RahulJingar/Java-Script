1.
let arr = [1,2,4,5,-9,-8,1,5,-8];
let ans =  arr.filter(function(num){
    return num>0
})
console.log(ans);
//output:[1, 2, 4, 5, 1, 5]

2.
let arr = [6,-5,-4,-8,6,-5,4,-1];
let ans = arr.filter(function(num){
    return num<0;
})
console.log(ans);
//output:[-5,-4,-8,-5,-1]

3.
let arr = [5,-4,9,-8,6,5,-9,0,9,-4,8,4,-9,0,5,-4];
let ans = arr.filter(function(num){
    return num===0;
})
console.log(ans);
//output:[0,0]
